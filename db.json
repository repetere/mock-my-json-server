                    }
                  ]
                },
                { 
                  "component":"li",
                  "children":[ 
                    { 
                      "component":"Link",
                      "props":{ 
                        "to":"/page/2"
                      },
                      "children":"Page two"
                    }
                  ]
                }
              ]
            }
          ]
        },
        "resources":{ 
          "post":"https://jsonplaceholder.typicode.com/posts/:id"
        },
        "pageData":[ 
          { 
            "tagName":"title",
            "attributes":{ 

            },
            "innerHTML":"My Dynamic Post - :id"
          }
        ]
      }
    }
  }
}
